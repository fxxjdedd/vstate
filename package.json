    "@ls-lint/ls-lint": "^1.8.0",
    "@microsoft/api-extractor": "^7.3.9",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.1",
    "@types/jest": "^26.0.0",
    "@types/node": "13.11.1",
    "brotli": "^1.3.2",
    "chalk": "^4.1.0",
    "conventional-changelog-cli": "^2.0.31",
    "csstype": "^2.6.8",
    "enquirer": "^2.3.2",
    "eslint": "^7.2.0",
